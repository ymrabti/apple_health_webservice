{
    "name": "health-web-server",
    "version": "1.7.0",
    "description": "Health check system, using Node.js, Express, MongoDB, Mongoose, JWT, Passport, Joi, Swagger, Docker, Jest, Travis, Coveralls, ESLint, PM2, Twilio, Socket.io, Winston, Sequelize",
    "main": "src/index.js",
    "license": "MIT",
    "engines": {
        "node": ">=12.0.0"
    },
    "prisma": {
        "schema": "prisma/schema.prisma"
    },
    "scripts": {
        "build": "webpack",
        "start": "pm2 start ecosystem.config.json --no-daemon",
        "dev": "cross-env nodemon src/index.js",
        "test": "jest -i --colors --verbose --detectOpenHandles",
        "test:watch": "jest -i --watchAll",
        "coverage": "jest -i --coverage",
        "coverage:coveralls": "jest -i --coverage --coverageReporters=text-lcov | coveralls",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "sequelize-automate": "node src/utils/Automate.js",
        "migration:generate": "sequelize-cli db:migrate",
        "migration:generate:dev": "sequelize-cli db:migrate --env development",
        "prisma:reset:local": "dotenv -e .env.local -- prisma migrate reset",
        "prisma:reset:staging": "dotenv -e .env.staging -- prisma migrate reset",
        "prisma:reset:prod": "dotenv -e .env.prod -- prisma migrate reset",
        "prisma:migrate:local": "dotenv -e .env.local -- prisma migrate dev",
        "prisma:migrate:staging": "dotenv -e .env.staging -- prisma migrate dev",
        "prisma:migrate:prod": "dotenv -e .env.prod -- prisma migrate deploy",
        "prisma:generate": "prisma generate",
        "prisma:studio:local": "dotenv -e .env.local -- prisma studio",
        "prisma:studio:staging": "dotenv -e .env.staging -- prisma studio",
        "prisma:studio:prod": "dotenv -e .env.prod -- prisma studio",
        "db:local": "yarn prisma:reset:local && yarn prisma:generate && yarn prisma:migrate:local",
        "db:staging": "yarn prisma:reset:staging && yarn prisma:generate && yarn prisma:migrate:staging",
        "db:prod": "yarn prisma:generate && yarn prisma:migrate:prod",
        "docker:prod": "docker-compose -f docker-compose.yml up -d --build",
        "docker:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
        "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up",
        "docker:down:prod": "docker-compose -f docker-compose.yml down -v",
        "docker:down:dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml down -v",
        "docker:down:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml down -v",
        "re:docker": "concurrently \"yarn docker:down:prod\" \"yarn build\" \"yarn docker:prod\""
    },
    "keywords": [
        "node",
        "node.js",
        "boilerplate",
        "generator",
        "express",
        "rest",
        "api",
        "mongodb",
        "mysql",
        "es6",
        "es7",
        "es8",
        "es9",
        "jest",
        "travis",
        "docker",
        "passport",
        "joi",
        "eslint"
    ],
    "dependencies": {
        "@prisma/client": "^7.2.0",
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "cookie-parser": "^1.4.7",
        "cors": "^2.8.5",
        "cross-env": "^7.0.0",
        "dayjs": "^1.11.13",
        "dotenv": "^10.0.0",
        "express": "^4.17.1",
        "express-mongo-sanitize": "^2.0.0",
        "express-rate-limit": "^5.0.0",
        "firebase-admin": "^13.3.0",
        "formidable": "^3.5.2",
        "helmet": "^4.1.0",
        "http-status": "^1.4.0",
        "joi": "^17.3.0",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.24.0",
        "morgan": "^1.9.1",
        "multer": "^1.4.5-lts.1",
        "mysql2": "^3.14.1",
        "nodemailer": "^6.10.1",
        "passport": "^0.4.0",
        "passport-cookie": "^1.0.9",
        "passport-jwt": "^4.0.0",
        "pm2": "6.0.5",
        "rotating-file-stream": "^3.2.7",
        "sequelize": "^6.37.7",
        "socket.io": "^4.8.1",
        "swagger-jsdoc": "^6.2.8",
        "swagger-ui-express": "^5.0.1",
        "unzipper": "^0.12.3",
        "uuid": "^11.1.0",
        "validator": "^13.0.0",
        "webpack": "^5.99.9",
        "webpack-cli": "^6.0.1",
        "webpack-node-externals": "^3.0.0",
        "winston": "^3.2.1",
        "xss-clean": "^0.1.1"
    },
    "devDependencies": {
        "concurrently": "^9.2.1",
        "coveralls": "^3.0.7",
        "dotenv-cli": "^11.0.0",
        "eslint": "^9.30.1",
        "node-mocks-http": "^1.8.0",
        "nodemon": "^2.0.0",
        "prisma": "^7.2.0",
        "sequelize-automate": "^1.2.2",
        "sequelize-cli": "6.6.3",
        "supertest": "^6.0.1"
    },
    "packageManager": "yarn@1.22.21+sha1.1959a18351b811cdeedbd484a8f86c3cc3bbaf72"
}
